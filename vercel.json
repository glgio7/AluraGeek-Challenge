{
	"version": 2,
	"builds": [
		{ "src": "server/index.ts", "use": "@vercel/node" },
		{ "src": "package.json", "use": "@vercel/static-build" }
	],
	"routes": [
		{ "handle": "filesystem" },
		{
			"src": "/api/(.*)",
			"dest": "server/index.ts"
		},
		{
			"src": "/(.*)",
			"dest": "/public/index.html"
		}
	]
}
