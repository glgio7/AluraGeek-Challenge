{
	"version": 2,
	"builds": [
		{
			"src": "client/package.json",
			"use": "@vercel/static-build"
		},
		{ "src": "server/src/index.ts", "use": "@vercel/node" }
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/index.html"
		},
		{
			"src": "/api/(.*)",
			"dest": "server/src/index.ts"
		}
	]
}
