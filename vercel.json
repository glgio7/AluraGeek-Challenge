{
	"version": 2,
	"builds": [
		{ "src": "server/src/index.ts", "use": "@vercel/node" },
		{
			"src": "client/src/main.tsx",
			"use": "@vercel/static-build"
		}
	],
	"routes": [
		{ "handle": "filesystem" },
		{
			"src": "/api/(.*)",
			"dest": "server/src/index.ts"
		},
		{
			"src": "/(.*)",
			"dest": "dist/index.html"
		}
	]
}
